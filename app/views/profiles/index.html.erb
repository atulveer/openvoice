<h1>Listing profiles</h1>

<table>
  <tr>
    <th>Voice</th>
    <th>Skype</th>
    <th>Sip</th>
    <th>INum</th>
    <th>Tropo</th>
    <th>Twitter</th>
    <th>Gtalk</th>
    <th>Call URL</th>
    <th>Voice Token</th>
    <th>Messaging Token</th>
  </tr>

<% @profiles.each do |profile| %>
  <tr>
    <td><%=h profile.voice %></td>
    <td><%=h profile.skype %></td>
    <td><%=h profile.sip %></td>
    <td><%=h profile.inum %></td>
    <td><%=h profile.tropo %></td>
    <td><%=h profile.twitter %></td>
    <td><%=h profile.gtalk %></td>
    <td><%=h profile.call_url %></td>
    <td><%=h profile.voice_token %></td>
    <td><%=h profile.messaging_token %></td>
    <td><%= link_to 'Show', user_profile_path(current_user, profile) %></td>
    <td><%= link_to 'Edit', edit_user_profile_path(current_user, profile) %></td>
    <td><%= link_to 'Destroy', user_profile_path(current_user, profile), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New profile', new_user_profile_path(current_user)%>